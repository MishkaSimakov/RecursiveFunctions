//
is_prime: (number: i64) -> b8 = {
    // 0 and 1 are not primes
    if (number < 2) {
        return false;
    }

    divider: i64 = 2;
    while (divider * divider <= number) {
        if (number % divider == 0) {
            return false;
        }

        divider = divider + 1;
    }

    return true;
}

main: () -> i64 = {
    current_num: i64 = 0;
    while (current_num < 50) {
        if (is_prime(current_num)) {
            output(current_num);
        }

        current_num = current_num + 1;
    }

    return 0;
}