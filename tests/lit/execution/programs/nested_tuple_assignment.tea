// RUN: %execute "%s" | %FileCheck %s

// CHECK:       1
// CHECK-NEXT:  2
// CHECK-NEXT:  3
// CHECK-NEXT:  4

extern println: (value: i64) -> ()

main: () -> i64 = {
    tuple: ((i64, i64), (i64, i64));

    tuple.0.0 = 1;
    tuple.0.1 = 2;
    tuple.1.0 = 3;
    tuple.1.1 = 4;

    println(tuple.0.0);
    println(tuple.0.1);
    println(tuple.1.0);
    println(tuple.1.1);

    return 0;
}